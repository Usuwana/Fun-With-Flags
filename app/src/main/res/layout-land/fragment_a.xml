<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_a">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="281dp">

        <ImageButton
            android:id="@+id/onecolumn"
            android:layout_width="42dp"
            android:layout_height="37dp"
            android:layout_marginStart="27dp"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="614dp"
            android:layout_marginRight="614dp"
            android:layout_marginBottom="7dp"
            android:src="@drawable/onesquare"
            app:layout_constraintBottom_toTopOf="@+id/twocolumn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/arrow"
            android:layout_width="42dp"
            android:layout_height="73dp"
            android:layout_marginStart="35dp"
            android:layout_marginLeft="35dp"
            android:layout_marginEnd="606dp"
            android:layout_marginRight="606dp"
            android:layout_marginBottom="19dp"
            android:src="@drawable/ar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/threecolumn"
            android:layout_width="50dp"
            android:layout_height="60dp"
            android:layout_marginStart="27dp"
            android:layout_marginLeft="27dp"
            android:layout_marginEnd="606dp"
            android:layout_marginRight="606dp"
            android:src="@drawable/three"
            app:layout_constraintBottom_toTopOf="@+id/arrow"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/twocolumn"
            android:layout_width="51dp"
            android:layout_height="59dp"
            android:layout_marginStart="26dp"
            android:layout_marginLeft="26dp"
            android:layout_marginEnd="606dp"
            android:layout_marginRight="606dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/two"
            app:layout_constraintBottom_toTopOf="@+id/threecolumn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>